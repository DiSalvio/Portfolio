$('ul.errors').html("")
<% if @project.errors.any? %>
  <% @project.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
  $('#project_form').html("").css({"border": "none", "padding": "0%"}).hide();
  $('#projects').show();
  $('.show_project').html('<%= j (render @project) %>')
<% end %>
